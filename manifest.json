{
  "manifest_version": 2,

  "name": "JIRA Task Copier",
  "description": "This extension allow you to copy key and value of selected issue on JIRA Agile Board.",
  "version": "1.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "icons": {
    "128": "icon_128.png",
    "16": "icon_16.png"
  },

  "permissions": [
    "tabs",
    "<all_urls>",
    "activeTab",
    "https://ajax.googleapis.com/",
    "clipboardRead",
    "clipboardWrite"
  ]
}
